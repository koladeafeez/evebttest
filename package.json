  "bugs": {
    "url": "https://github.com/Instiq-professionals/city-events-api/issues"
  },
  "homepage": "https://github.com/Instiq-professionals/city-events-api#readme",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chance": "^1.1.7",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "email-templates": "^7.1.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^5.1.3",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.11",
    "nodemailer-express-handlebars": "^4.0.0",
    "pug": "^3.0.0",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "winston": "^3.3.3"
  }
