{
  "name": "city-events-api",
  "version": "1.0.0",
  "description": "An API for City Events WebApp",
  "main": "index.js",
  "scripts": {
    "test": "npm test",
    "start:dev": "nodemon index.js",
    "start": "node index.js",
    "seq:init": "sequelize init",
    "migrate": "sequelize db:migrate",
    "migrate:create": "sequelize migration:create --name",
    "migrate:undo": "sequelize db:migrate:undo",
    "model:generate": "npx sequelize-cli model:generate --name AdminDiscountCouponUser --attributes eventCouponId:integer,attendeeId:integer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Instiq-professionals/city-events-api.git"
  },
